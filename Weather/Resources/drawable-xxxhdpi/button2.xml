<?xml version="1.0" encoding="utf-8" ?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_pressed="true" >
    <shape>
      <solid
          android:color="#ff22e6ff" />
      <stroke
          android:width="1dp"
          android:color="#ff6fefff" />
      <corners
          android:radius="30dp" />
      <padding
          android:left="10dp"
          android:top="10dp"
          android:right="10dp"
          android:bottom="10dp" />
    </shape>
  </item>
  <item android:state_enabled="true">
    <shape>
      <solid
          android:color="#ff00bcd4" />
      <stroke
          android:width="1dp"
          android:color="#ff22e6ff" />
      <corners
          android:radius="30dp" />
      <padding
          android:left="10dp"
          android:top="10dp"
          android:right="10dp"
          android:bottom="10dp" />
    </shape>
  </item>
  <item android:state_enabled="false" >
    <shape>
      <solid
          android:color="#ff0a5c67" />
      <stroke
          android:width="1dp"
          android:color="#ff00bcd4" />
      <corners
          android:radius="30dp" />
      <padding
          android:left="10dp"
          android:top="10dp"
          android:right="10dp"
          android:bottom="10dp" />
    </shape>
  </item>
</selector>